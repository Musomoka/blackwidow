<p id="notice"><%= notice %></p>

<h1>Businesses</h1>

<table>
  <thead>
    <tr>
      <th>Businessname</th>
      <th>Description</th>
      <th>Location</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.businessName %></td>
        <td><%= business.description %></td>
        <td><%= business.location %></td>
        <td><%= business.owner_id %></td>
        <td><%= link_to 'Show', business %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

